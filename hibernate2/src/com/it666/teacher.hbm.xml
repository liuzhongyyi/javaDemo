<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    <class name="com.it666.Teacher" table="teacher">
    	<id name="tid" column="tid">
    	<generator class="native"></generator>
    	</id>
    	<property name="tname"></property>
    	<property name="tage"></property>
    	<set name="student" cascade="delete-orphan" table="zhongjian">
    		<key column="teacher_id"></key>
    		<many-to-many class="com.it666.Student" column="student_id" ></many-to-many>
    	</set>
    </class>
    
    </hibernate-mapping>